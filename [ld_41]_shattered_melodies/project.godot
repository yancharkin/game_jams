; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

[application]

config/name="Shattered Melodies"
run/main_scene="res://menus/MainMenu/MainMenu.tscn"
boot_splash/image="res://images/icon-192.png"
boot_splash/fullsize=false
boot_splash/use_filter=false
boot_splash/bg_color=Color( 0.133333, 0.12549, 0.203922, 1 )
config/icon="res://images/icon.png"
config/windows_native_icon="res://images/icon.ico"

[autoload]

_Config="*res://scripts/_Config.gd"
_Scene="*res://scripts/_Scene.gd"
_Globals="*res://scripts/_Globals.gd"
_Player="*res://scripts/_Player.gd"
_Camera="*res://scripts/_Camera.gd"
_Enemies="*res://scripts/_Enemies.gd"
_Platform="*res://scripts/_Platform.gd"
HUD="*res://hud/HUD/HUD.tscn"

[display]

window/size/width=640
window/size/height=360
window/size/fullscreen=true
window/stretch/mode="2d"
window/stretch/aspect="expand"

[editor_plugins]

enabled=PoolStringArray( "res://addons/vnen.tiled_importer/plugin.cfg" )

[global]

export=false

[importer_defaults]

texture={
